{
    "name": "typescript-node-base",
    "version": "0.1.0",
    "description": "",
    "main": "dist/index.js",
    "type": "module",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "node": "node .",
    "start:production": "node dist/index.js",
    "dev:nodemon": "nodemon -x ts-node src/index.ts",
    "postinstall": "npm run build",
    "m:generate": "npx ts-node node_modules/.bin/typeorm migration:generate -n",
    "m:run": "npx ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:prod": "ts-node node_modules/.bin/typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.6",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.2.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^3.9.7"
  }
}
